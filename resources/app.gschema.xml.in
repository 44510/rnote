<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="@APP_ID@" path="@APP_IDPATH@">
    <key name="window-width" type="i">
      <default>-1</default>
      <summary>Default window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>-1</default>
      <summary>Default window height</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Default window maximized behaviour</summary>
    </key>
    <key name="devel" type="b">
      <default>false</default>
      <summary>Enable the developer mode in the application</summary>
    </key>
    <key name="visual-debug" type="b">
      <default>false</default>
      <summary>Enable visual debugging</summary>
    </key>
    <key name="renderer-backend" type="s">
      <default>"librsvg"</default>
      <summary>the renderer backend</summary>
    </key>
    <key name="color-scheme" type="s">
      <default>"default"</default>
      <summary>Color scheme preference</summary>
    </key>
    <key name="touch-drawing" type="b">
      <default>false</default>
      <summary>Enable touch input for drawing</summary>
    </key>
    <key name="righthanded" type="b">
      <default>true</default>
      <summary>user is right or left handed</summary>
    </key>
    <key name="autoexpand-height" type="b">
      <default>false</default>
      <summary>Autoexpand sheet height ( also called "endless sheet" )</summary>
    </key>
    <key name="format-borders" type="b">
      <default>false</default>
      <summary>Show sheet format borders</summary>
    </key>
    <key name="workspace-dir" type="s">
      <default>"./"</default>
      <summary>Workspace directory</summary>
    </key>
    <key name="marker-width" type="d">
      <default>2</default>
      <summary>Marker width in px</summary>
    </key>
    <key name="marker-colors" type="(uuuuuuuu)">
      <default>(
        0x000000ff,
        0xffffffff,
        0x99c1f1ff,
        0x1a5fb4ff,
        0x26a269ff,
        0xf6d32dff,
        0xff7800ff,
        0xe01b24ff
      )</default>
      <summary>Marker colors encoded as RGBA values (8-bit)</summary>
    </key>
    <key name="brush-width" type="d">
      <default>6</default>
      <summary>Brush width in px</summary>
    </key>
    <key name="brush-colors" type="(uuuuuuuu)">
      <default>(
        0x000000ff,
        0xffffffff,
        0x99c1f1ff,
        0x1a5fb4ff,
        0x26a269ff,
        0xf6d32dff,
        0xff7800ff,
        0xe01b24ff
      )</default>
      <summary>Brush colors encoded as RGBA values (8-bit)</summary>
    </key>
    <key name="shaper-width" type="d">
      <default>2</default>
      <summary>Brush width in px</summary>
    </key>
    <key name="shaper-stroke-colors" type="(uu)">
      <default>(
        0x000000ff,
        0x1a5fb4ff
      )</default>
      <summary>Shape stroke colors encoded as RGBA values (8-bit)</summary>
    </key>
    <key name="shaper-fill-colors" type="(uu)">
      <default>(
        0x00000000,
        0x62a0ea99
      )</default>
      <summary>Shape fill colors encoded as RGBA values (8-bit)</summary>
    </key>
    <key name="eraser-width" type="d">
      <default>30</default>
      <summary>Eraser width in px</summary>
    </key>
    <key name="format-size" type="(uu)">
      <default>(1240, 1754)</default>
      <summary>format size in (width, height)</summary>
    </key>
    <key name="format-dpi" type="d">
      <default>96</default>
      <summary>Format DPI</summary>
    </key>
    <key name="background-color" type="u">
      <default>0xffffffff</default>
      <summary>Background color encoded as RGBA value (8-bit)</summary>
    </key>
    <key name="background-pattern" type="s">
      <default>"None"</default>
      <summary>Background pattern</summary>
    </key>
    <key name="background-pattern-color" type="u">
      <default>0x99c1f1ff</default>
      <summary>Background pattern color encoded as RGBA value (8-bit)</summary>
    </key>
    <key name="background-pattern-size" type="(uu)">
      <default>(32, 32)</default>
      <summary>Background pattern size</summary>
    </key>
  </schema>
</schemalist>